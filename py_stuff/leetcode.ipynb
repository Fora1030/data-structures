    "class Palindrome(object):\n",
    "    def first_palindrome(self, words):\n",
    "        for word in words:\n",
    "            if word == word[::-1]:\n",
    "                return word\n",
    "        return \"\"\n",
    "\n",
    "\n",
    "Palindrome().first_palindrome(\n",
    "    [\"racecar\", \"dog\", \"apple\", \"racecar\", \"dog\", \"apple\"])"
   ]
  }
